{
  "name": "@peardeck/uikit-icons",
  "version": "0.0.3",
  "description": "> TODO: description",
  "author": "Pear Deck",
  "license": "MIT",
  "main": "dist/index.js",
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/peardeck/peardeck-uikit.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "build:all": "yarn run clean && yarn run build:components && yarn run build:index && yarn run compile",
    "build:components": "svgr --ext tsx -d ./src/transformed ./svgs",
    "build:index": "ts-node --skip-project ./scripts/build-index.ts",
    "compile": "tsc",
    "watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "yarn run lint --fix",
    "clean": "rimraf dist",
    "test:snapshot": "jest --updateSnapshot",
    "precommit": "lint-staged"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "styled-components": "^4.0.0"
  },
  "devDependencies": {
    "@peardeck/eslint-config": "^0.0.2",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@svgr/babel-plugin-svg-dynamic-title": "^5.0.1",
    "@svgr/cli": "^5.3.0",
    "@types/jest": "^25.1.5",
    "@types/react": "^16.9.32",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^26.0.1",
    "jest-styled-components": "^7.0.0",
    "lint-staged": "^10.1.1",
    "prettier": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.0.1",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
