{
  "name": "@peardeck/uikit-icons",
  "version": "0.0.1",
  "description": "> TODO: description",
  "author": "Pear Deck",
  "license": "MIT",
  "main": "dist/index.js",
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/peardeck/peardeck-uikit.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "build:all": "yarn run clean && yarn run build:components && yarn run build:index && yarn run compile",
    "build:components": "svgr --ext tsx -d ./src/transformed ./svgs",
    "build:index": "ts-node --skip-project ./scripts/build-index.ts",
    "compile": "tsc",
    "watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "yarn run lint --fix",
    "clean": "rimraf dist",
    "test": "jest",
    "test:snapshot": "jest --updateSnapshot",
    "precommit": "lint-staged"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "styled-components": "^4.0.0"
  },
  "devDependencies": {
    "@peardeck/eslint-config": "^0.0.1",
    "@storybook/addon-knobs": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "@svgr/babel-plugin-svg-dynamic-title": "^4.3.3",
    "@svgr/cli": "^4.3.3",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.9.5",
    "@types/react-test-renderer": "^16.9.0",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "eslint": "^6.5.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^24.8.0",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-test-renderer": "^16.10.2",
    "rimraf": "^3.0.0",
    "styled-components": "^4.4.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
