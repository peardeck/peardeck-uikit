    "compile": "tsc",
    "build:watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "clean": "rimraf lib",
    "start": "yarn run build:watch",
    "test": "jest",
    "test:snapshot": "jest --updateSnapshot"
  },
